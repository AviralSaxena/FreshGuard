{
  "expo": {
    "name": "FreshGuard",
    "slug": "FreshGuard",
    "version": "1.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "freshguard",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.nguyen2705.FreshGuard",
      "supportsTablet": true,
      "buildNumber": "1", 
      "googleServicesFile": "./ios/GoogleService-Info.plist"
    },
    "android": {
      "package": "com.nguyen2705.FreshGuard", 
      "googleServicesFile": "./android/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1eb13e78-9032-464b-ae3a-1bf6ce79e26f"
      }
    }
  }
}
